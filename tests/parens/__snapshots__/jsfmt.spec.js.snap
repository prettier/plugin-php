    echo $i++;
}

do {
    echo $i;
} while ($i > 0);

switch ($i) {
    case 0:
        echo "i equals 0";
        break;
    case 1:
        echo "i equals 1";
        break;
    case 'test' . (1 > 2 ? 'foo' : 'bar'):
        echo "i equals 2";
        break;
}

switch ($i + 1) {
    case 0:
        echo "i equals 0";
        break;
    case 1 + 2:
        echo "i equals 1";
        break;
    case 1 + (1 > 2 ? 1 : 3):
        echo "i equals 2";
        break;
}

while (++$i) {
    switch ($i) {
        case 5:
            echo "At 5<br />\\n";
            break 2; /* Exit only the switch. */
        case 10:
            echo "At 10; quitting<br />\\n";
            break 4; /* Exit the switch and the while. */
